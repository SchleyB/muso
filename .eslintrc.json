{
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "describe": true,
    "it": true,
    "done": true,
    "beforeEach": true,
    "afterEach": true
  },
  "extends": "airbnb",
  "installedESLint": true,
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/prop-types": [2, {"skipUndeclared": true}],
    "no-console": 0,
    "max-len": [2, {"code": 120}],
    "react/jsx-no-bind": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".jsx"]  }],
    "indent": ["error", 2, { "SwitchCase": 1 }],
    "jsx-a11y/no-static-element-interactions": 0,
    "import/extensions": 0
  }
}
